{
  "name": "wiremock-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:server & npm run start:client",
    "start:client": "vue-cli-service serve",
    "start:server": "nodemon src/server/main.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.6",
    "dotenv": "^5.0.1",
    "fs-extra": "^5.0.0",
    "http-status-codes": "^1.3.0",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.2.1",
    "vue": "^2.5.13",
    "vue-awesome": "^3.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "sass-loader": "^6.0.7",
    "sass-resources-loader": "^1.3.3",
    "valid-url": "^1.0.9",
    "vue-template-compiler": "^2.5.13",
    "vuelidate": "^0.6.2"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "nodemonConfig": {
    "ignore": [
      "src/client/*"
    ],
    "delay": "500"
  }
}
